{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["!node_modules/**", "!src/**", "*/**", "packages/medusa/dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test:integration": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "test:integration:chunk": {
      "dependsOn": ["build"],
      "outputs": []
    }
  },
  "globalDependencies": ["turbo.json"]
}
